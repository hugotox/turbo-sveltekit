{
  "name": "web",
  "version": "1.0.0",
  "description": "web application",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite dev --port 5175",
    "build": "vite build",
    "check-updates": "npx npm-check-updates",
    "preview": "env-cmd -f ../../.env vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "lint": "npm run prettier -- --check . && eslint .",
    "format": "npm run prettier -- --write .",
    "prettier": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss",
    "update-all": "npx npm-check-updates -u"
  },
  "dependencies": {
    "backend": "*",
    "ui": "*"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.1",
    "@sveltejs/kit": "^2.0.4",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/node": "^20.10.5",
    "autoprefixer": "^10.4.16",
    "bits-ui": "^0.11.8",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "drizzle-orm": "^0.29.1",
    "eslint": "^8.56.0",
    "eslint-config-custom": "*",
    "lucia": "^2.7.5",
    "lucide-svelte": "^0.298.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tailwind-merge": "^2.1.0",
    "tailwind-variants": "^0.1.19",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-tailwind-purgecss": "^0.2.0",
    "vitest": "^1.1.0"
  },
  "type": "module"
}
